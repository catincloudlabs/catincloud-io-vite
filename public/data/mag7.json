    "inspector": {
      "tag": "Trend",
      "description": "Net Sentiment Flow (Call Delta - Put Delta) over the last 30 days.",
      "sql_logic": "\nSELECT \n    TRADE_DATE,\n    TICKER,\n    NET_SENTIMENT_FLOW\nFROM STOCKS_ELT_DB.PREP.int_massive__mag7_momentum\nWHERE TRADE_DATE >= DATEADD('day', -30, CURRENT_DATE())\nORDER BY TRADE_DATE ASC\n",
      "dbt_logic": "-- models/intermediate/int_massive__mag7_momentum.sql\n\n{{ config(\n    materialized = 'incremental',\n    unique_key = 'daily_id',\n    cluster_by = ['trade_date']\n) }}\n\nwith source as (\n    select * from {{ ref('int_massive__stocks_options_joined') }}\n\n    {% if is_incremental() %}\n      -- CRITICAL: Only process rows added since the last run.\n      -- No buffer needed here because the hard math is done upstream.\n      where trade_date >= (select max(trade_date) from {{ this }})\n    {% endif %}\n)\n\nselect\n    {{ dbt_utils.generate_surrogate_key(['trade_date', 'underlying_ticker']) }} as daily_id,\n    trade_date,\n    underlying_ticker as ticker,\n    sum(\n        case when option_type = 'C' then (option_volume * option_close_price * 100)\n             else -(option_volume * option_close_price * 100)\n        end\n    ) as net_sentiment_flow\nfrom source\nwhere \n    ticker_category = 'Mag 7'\ngroup by 1, 2, 3\n",
      "dbt_yml": "\n  - name: int_massive__stocks_options_joined\n    description: >\n      The Central Truth. Joins de-duped Options data with de-duped Underlying Stock data.\n      [INCREMENTAL TABLE]: Materializes the join daily to prevent massive history scans.\n    tags: [\"intermediate\", \"joined\", \"core\", \"incremental\"]\n    columns:\n      - name: id\n        tests: [unique, not_null]\n      - name: ticker_category\n        description: \"Classification: 'Mag 7', 'Chaos', or 'Other'.\"\n      - name: vol_risk_premium\n        description: \"Alpha Signal: Spread between IV and 20-day Realized Volatility.\"\n\n  - name: int_massive__mag7_momentum\n    description: >\n      Feeds the 'Mag 7 Momentum' Time Series Chart.\n      [INCREMENTAL]: Aggregates daily Net Sentiment Flow for the Magnificent 7 tickers.\n    tags: [\"dashboard\", \"trend\", \"mag7\", \"incremental\"]\n    columns:\n      - name: daily_id\n        description: \"Surrogate Key: hash(trade_date, ticker)\"\n        tests: [unique, not_null]\n      - name: net_sentiment_flow\n        description: >\n          The Delta-Adjusted Money Flow.\n          Calculation: Sum((Call Vol * Price) - (Put Vol * Price)).\n"
    }
  },
